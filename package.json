{
  "name": "suppress-exit-code",
  "version": "1.0.0",
  "description": "Cross-platform CLI wrapper that runs any command and exits with zero",
  "keywords": [
    "cli",
    "exit-code",
    "ignore",
    "suppress"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kachkaev/suppress-exit-code.git"
  },
  "license": "MIT",
  "author": {
    "name": "Alexander Kachkaev",
    "url": "https://en.kachkaev.ru/"
  },
  "main": "index.js",
  "bin": "index.js",
  "files": [
    "index.js",
    "README.md"
  ],
  "scripts": {
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-dedupe": "yarn dedupe",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-dedupe": "yarn dedupe --check",
    "prepublishOnly": "yarn lint && yarn test",
    "test": "jest"
  },
  "dependencies": {
    "execa": "^5.0.0"
  },
  "devDependencies": {
    "@kachkaev/prettier-config": "^1.2.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "jest": "^27.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "typescript": "^4.5.2"
  },
  "packageManager": "yarn@4.0.0-rc.26"
}
