{
  "name": "suppress-exit-code",
  "version": "0.1.0",
  "description": "Cross-platform CLI wrapper that runs any command and exits with zero",
  "keywords": [
    "cli",
    "exit-code",
    "ignore",
    "suppress"
  ],
  "homepage": "https://github.com/kachkaev/suppress-exit-code",
  "repository": "kachkaev/suppress-exit-code",
  "license": "MIT",
  "author": {
    "name": "Alexander Kachkaev",
    "url": "https://en.kachkaev.ru/"
  },
  "main": "index.js",
  "bin": {
    "suppress-exit-code": "index.js"
  },
  "files": [
    "index.js",
    "README.md"
  ],
  "scripts": {
    "fix": "npm-run-all --continue-on-error fix:*",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-deduplicate": "yarn install && yarn-deduplicate --strategy=fewer && yarn install",
    "lint": "npm-run-all --continue-on-error lint:*",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer"
  },
  "dependencies": {
    "execa": "^5.0.0"
  },
  "devDependencies": {
    "@kachkaev/prettier-config": "^1.2.0",
    "@types/execa": "^2.0.0",
    "@types/node": "^14.14.14",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "tsc": "^1.20150623.0",
    "typescript": "^4.1.3",
    "yarn-deduplicate": "^3.1.0"
  }
}
