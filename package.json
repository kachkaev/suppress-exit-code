{
  "name": "suppress-exit-code",
  "version": "2.0.1",
  "description": "Cross-platform CLI wrapper that runs any command and exits with zero",
  "keywords": [
    "cli",
    "exit-code",
    "ignore",
    "suppress"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kachkaev/suppress-exit-code.git"
  },
  "license": "MIT",
  "author": {
    "name": "Alexander Kachkaev",
    "url": "https://en.kachkaev.ru/"
  },
  "type": "module",
  "main": "main.js",
  "bin": "main.js",
  "scripts": {
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:prettier": "prettier --write .",
    "fix:yarn-dedupe": "yarn dedupe",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --project .",
    "lint:yarn-dedupe": "yarn dedupe --check",
    "prepack": "yarn lint && yarn test && pinst --disable",
    "postpack": "pinst --enable",
    "postinstall": "husky install",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)"
  },
  "lint-staged": {
    "**": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "execa": "^6.1.0"
  },
  "devDependencies": {
    "@kachkaev/prettier-config": "1.3.1",
    "@types/jest": "29.2.0",
    "@types/node": "18.11.2",
    "husky": "8.0.1",
    "jest": "29.2.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@4.0.0-rc.26",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  }
}
